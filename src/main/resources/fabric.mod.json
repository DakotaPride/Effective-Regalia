  "authors": [
    "DakotaPride (Author)",
    "HoloScopeStudios (Mascot)"
  ],
  "contact": {
    "homepage": "www.curseforge.com/minecraft/mc-mods/effective-regalia",
    "sources": "https://github.com/DakotaPride/effective-regalia",
    "issues": "https://github.com/DakotaPride/effective-regalia/issues"
  },

  "license": "MIT",
  "icon": "assets/effectiveregalia/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.dakotapride.effectiveRegalia.common.EffectiveRegalia"
    ],
    "fabric-datagen": [
      "net.dakotapride.effectiveRegalia.common.register.data.DataGenProvider"
    ]
  },
  "mixins": [
    "effectiveregalia.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.21",
    "fabric-api": "*",
    "minecraft": ">=1.19.3",
    "java": ">=17",
    "trinkets": ">=3.5.0"
  }
